<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>{{title}}</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header}}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Meals</h1>
							<ul id="foods">
			{{#each food}}
								<li>
									<h2><a href={{this.link}}>{{this.title}}</a></h2>
									<div class="infos">
										<a href={{this.link}}><img src="images/{{this.image}}" alt="Img" height="169" width="780"><span class="cover"></span></a>
										{{{this.description}}}
									</div>
								</li>
			{{/each}}
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer}}
	</div>
</body>
</html>